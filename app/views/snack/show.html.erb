<h1><%= @snack.name %></h1>
<h2><%= @snack.price %></h2>
<section class="snack-locations">
  <h3>Available at:</h3>
  <% @snack.machines.each do |machine| %>
    <p><%= machine.location %> (<%= machine.snacks.count %> kinds of snacks, average price of $<%= machine.average_snack_price %>) /p>
  <% end %>
</section>
